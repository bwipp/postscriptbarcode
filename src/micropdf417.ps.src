/micropdf417.latevars dup 8 dict def load /init {

    (setup.latevars) //micropdf417.before exec

    currentglobal
    true setglobal

    //micropdf417.latevars begin

    %
    % Invert charmaps to give character to value maps for each state
    %
    /charvals [ 30 dict 30 dict 30 dict 30 dict ] def
    /alltext 104 dict def
    0 1 //micropdf417.charmaps length 1 sub {
        /i exch def
        /encs //micropdf417.charmaps i get def
        0 1 3 {
            /j exch def
            encs j get dup type /stringtype eq {0 get} if
            dup charvals j get exch i put
            alltext exch -1 put
        } for
    } for
    /charvals charvals readonly def
    /alltext alltext readonly def

    %
    % Calculate the Reed-Solomon log and anti-log tables
    %
    /rsalog [ 1 928 {dup 3 mul 929 mod} repeat ] readonly def
    /rslog 929 array def
    1 1 928 {dup rsalog exch get exch rslog 3 1 roll put} for
    /rslog rslog readonly def

    /init { //micropdf417.latevars {def} forall } def

    end

    //micropdf417.latevars /init get exec

    setglobal

    (setup.latevars) //micropdf417.after exec

} bind put

/uk.co.terryburton.bwipp.global_ctx dup where {
    exch get /preload known {//micropdf417.latevars /init get exec} if
} {pop} ifelse

%
% Function to calculate the product in the field
%
/micropdf417.rsprod {
    dup 0 ne { 1 index 0 ne {
        rslog exch get exch rslog exch get add 928 mod rsalog exch get
    } { pop pop 0 } ifelse } { pop pop 0 } ifelse
} bind def

%
% Generate Reed-Solomon coefficients for a given k
%
/micropdf417.gencoeffs {
    /coeffs [ 1 k {0} repeat ] def
    1 1 k {  % i
        coeffs 1 index 2 copy 1 sub get put      % coeffs[i] = coeffs[i-1]
        rsalog 1 index get                       % ai = rsalog[i]
        exch 1 sub -1 1 {  % j
            coeffs exch 2 copy get               % ... coeffs j coeffs[j]
            3 index                              % ... ai
            //micropdf417.rsprod exec            % p = coeffs[j] * ai
            coeffs 2 index 1 sub get             % ... coeffs[j-1]
            add 929 mod put                      % coeffs[j] = (p + coeffs[j-1]) mod 929
        } for
        coeffs 0 2 copy get                      % ... coeffs 0 coeffs[0]
        3 index                                  % ... ai
        //micropdf417.rsprod exec put            % coeffs[0] = coeffs[0] * ai
        pop  % ai
    } for
    /coeffs coeffs 0 coeffs length 1 sub getinterval def
    coeffs length 1 sub -2 0 { coeffs exch 2 copy get 929 exch sub put } for
    coeffs
} bind def

%
% FIFO cache for ECC polynomials
%
% 20 unique keys: 4-50 coefficients from symbol metrics
%
/micropdf417.coeffscachemax 20 def    % Override with global_ctx.micropdf417.coeffscachemax
/micropdf417.coeffscachelimit 359 def % Override with global_ctx.micropdf417.coeffscachelimit
/uk.co.terryburton.bwipp.global_ctx dup where {
    exch get
    dup /micropdf417.coeffscachemax 2 copy known {get /micropdf417.coeffscachemax exch def} {pop pop} ifelse
    /micropdf417.coeffscachelimit 2 copy known {get /micropdf417.coeffscachelimit exch def} {pop pop} ifelse
} {pop} ifelse

/micropdf417.coeffscache micropdf417.coeffscachemax micropdf417.coeffscachelimit //fifocache exec def

/micropdf417.coeffscachefetch {
    /k exch def
    k
    { //micropdf417.gencoeffs exec }
    { length }
    //micropdf417.coeffscache /fetch get exec
} bind def

/micropdf417 {

    128 dict begin

    (input.processoptions) //micropdf417.before exec

    /dontdraw false def
    /version (unset) def
    /columns 0 def
    /rows 0 def
    /rowmult 2 def
    /cca false def
    /ccb false def
    /raw false def
    /parse false def
    /parsefnc false def

    //processoptions exec /options exch def
    /barcode exch def

    /_render
        dontdraw /uk.co.terryburton.bwipp.global_ctx dup where {exch get /enabledontdraw known} {pop false} ifelse and
        /uk.co.terryburton.bwipp._dontdraw dup where {exch get} {pop false} ifelse or not
    def

    (input.processoptions) //micropdf417.after exec

    //micropdf417.latevars /init get exec

    (input.validate) //micropdf417.before exec

    barcode () eq {
        /bwipp.micropdf417emptyData (The data must not be empty) //raiseerror exec
    } if

    rowmult 0 le rowmult 50 gt or {
        /bwipp.micropdf417badRowMult (The row multiplier must be greater than 0 and at most 50) //raiseerror exec
    } if

    cca ccb and cca raw and ccb raw and or or {
        /bwipp.micropdf417ccaAndCcb (Cannot combine cca, ccb and raw) //raiseerror exec
    } if

    %
    % Parse version=RxC into rows and columns
    %
    version (unset) ne {
        version (x) search {
            exch pop
            2 copy 2 array astore {
                true
                1 index length 0 eq { pop false } if
                exch {
                    dup 48 lt exch 57 gt or { pop false } if
                } forall
                not {
                    pop pop /bwipp.micropdf417BadVersionBadRowOrColumn (version must be formatted as RxC) //raiseerror exec
                } if
            } forall
            /rows exch cvi def
            /columns exch cvi def
        } {
            pop /bwipp.micropdf417badVersionFormat (version must be formatted as RxC) //raiseerror exec
        } ifelse
    } if

    (input.validate) //micropdf417.after exec

    %
    % Convert input into array of codewords
    %
    raw cca or {
        /datcws barcode length array def
        /i 0 def /j 0 def
        { % loop
            i barcode length 3 sub ge {exit} if
            barcode i get 94 ne {exit} if
            barcode i 1 add 3 getinterval false 1 index {
                dup 48 lt exch 57 gt or { pop true } if
            } forall { pop exit } if
            cvi /cw exch def
            datcws j cw put
            /i i 4 add def
            /j j 1 add def
        } loop
        i barcode length ne {
            /bwipp.micropdf417badCcaRawFormat (cca and raw data must be formatted as ^NNN) //raiseerror exec
        } if
        /datcws datcws 0 j getinterval def
    } if

    /encb {
        /in exch def
        /inlen in length def
        /out inlen 6 idiv 5 mul inlen 6 mod add array def
        0 1 inlen 6 idiv 1 sub {
            /k exch def
            /msbs [ in k 6 mul 3 getinterval aload pop ] def
            /mscs [
                msbs aload pop exch 256 mul add exch 65536 mul add
                3 {dup 900 mod exch 900 idiv} repeat
            ] def
            /lsbs [ in k 6 mul 3 add 3 getinterval aload pop ] def
            /lscs [
                lsbs aload pop exch 256 mul add exch 65536 mul add
                3 {dup 900 mod exch 900 idiv} repeat
            ] def
            lscs 0 get mscs 0 get 316 mul add
            out k 5 mul 4 add 2 index 900 mod put
            900 idiv lscs 1 get add mscs 0 get 641 mul add mscs 1 get 316 mul add
            out k 5 mul 3 add 2 index 900 mod put
            900 idiv lscs 2 get add mscs 0 get 20 mul add mscs 1 get 641 mul add mscs 2 get 316 mul add
            out k 5 mul 2 add 2 index 900 mod put
            900 idiv lscs 3 get add mscs 1 get 20 mul add mscs 2 get 641 mul add
            out k 5 mul 1 add 2 index 900 mod put
            900 idiv mscs 2 get 20 mul add
            out k 5 mul 3 -1 roll 900 mod put
        } for
        /rem inlen 6 mod def
        rem 0 ne {
            out out length rem sub
            [ in inlen rem sub rem getinterval aload pop ]
            putinterval
        } if
        out
    } def

    ccb {
        /barlen barcode length def
        /datcws barlen 6 idiv 5 mul barlen 6 mod add 2 add array def
        datcws 0 920 put
        datcws 1 barlen 6 mod 0 eq {924} {901} ifelse put
