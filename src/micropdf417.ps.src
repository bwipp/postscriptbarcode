            p 0 eq msg p get dup //micropdf417.m05 eq exch //micropdf417.m06 eq or and {
                msg p get //micropdf417.m05 eq { //micropdf417.m05 } { //micropdf417.m06 } ifelse
                /p p 1 add def
                /seqlen seqlen 1 add def
