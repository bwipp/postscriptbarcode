/micropdf417.e 10000 def  % "Empty"
/e //micropdf417.e def

%
% Bit length of latch between submodes
%
/micropdf417.latlen [
% To:  A  L  M  P      From
    [  0  1  1  2 ]  % A
    [  2  0  1  2 ]  % L
    [  1  1  0  1 ]  % M
    [  1  2  2  0 ]  % P
] readonly def

%
% Latch sequences between submodes
%
/micropdf417.latseq [
% To: A       L       M       P               From
    [ []      [ll]    [ml]    [ml pl]    ]  % A
    [ [ml al] []      [ml]    [ml pl]    ]  % L
    [ [al]    [ll]    []      [pl]       ]  % M
    [ [al]    [al ll] [al ml] []         ]  % P
] readonly def

%
% Bit length of shift to submode
%
/micropdf417.shftlen [
% To:  A  L  M  P      From
    [  e  e  e  1 ]  % A
    [  1  e  e  1 ]  % L
    [  e  e  e  1 ]  % M
    [  e  e  e  e ]  % P
] readonly def

