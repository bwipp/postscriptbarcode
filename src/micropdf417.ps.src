                                //micropdf417.latseq x get y get {
                                    curseq y get srclen 3 -1 roll put
                                    /srclen srclen 1 add def
                                } forall
                                curseqlen y srclen put
                                /imp true def
                            } if
                        } forall
                    } forall
                    imp not {exit} if  % Repeat unless no improvement
                } loop

                %
                % Determine optimal next sequences for each valid encoding
                %
                /nxtlen [ //micropdf417.e //micropdf417.e //micropdf417.e //micropdf417.e ] def
                nxtseqlen 0 0 put  nxtseqlen 1 0 put  nxtseqlen 2 0 put  nxtseqlen 3 0 put

                //micropdf417.textsubmodes {
                    /x exch def

                    { % loop for common exit

                        charvals x get char known not {exit} if

                        %
                        % Extend directly: copy curseq[x] + char to nxtseq[x]
                        %
                        /cost curlen x get 1 add def
                        cost nxtlen x get lt {
                            nxtlen x cost put
                            /srclen curseqlen x get def
                            srclen 0 gt {
                                nxtseq x get 0 curseq x get 0 srclen getinterval putinterval
                            } if
                            nxtseq x get srclen char put
                            nxtseqlen x srclen 1 add put
                        } if

                        %
                        % Optimise for direct shifts from y to x
                        %
                        //micropdf417.textsubmodes {
                            /y exch def
                            x y ne {
                                /cost curlen y get //micropdf417.shftlen y get x get add 1 add def
                                cost nxtlen y get lt {
                                    nxtlen y cost put
                                    % Copy curseq[y] + shift + char to nxtseq[y]
                                    /srclen curseqlen y get def
                                    srclen 0 gt {
                                        nxtseq y get 0 curseq y get 0 srclen getinterval putinterval
                                    } if
                                    nxtseq y get srclen x //micropdf417.a eq {//micropdf417.as} {//micropdf417.ps} ifelse put
                                    nxtseq y get srclen 1 add char put
                                    nxtseqlen y srclen 2 add put
                                } if
                            } if
                        } forall

                        exit
                    } loop

                } forall

                /curlen nxtlen def
                % Swap cur and nxt buffers
                curseq nxtseq /curseq exch def /nxtseq exch def
                curseqlen nxtseqlen /curseqlen exch def /nxtseqlen exch def

            } forall

            %
            % Select the optimal sequence
            %
            /minseq //micropdf417.e def
            //micropdf417.textsubmodes {
                /k exch def
                curlen k get minseq lt {
                    /minseq curlen k get def
                    /txtseq curseq k get 0 curseqlen k get getinterval def
                } if
            } forall

            %
            % Encode the sequence
            %
            /text minseq array def
            /k 0 def /l 0 def {
                k txtseq length ge {exit} if

                /char txtseq k get def

                %
                % Encode character
                %
                char textencfuncs submode get load exec addtotext
                /k k 1 add def

                %
                % Encode shifted next character
                %
                char //micropdf417.as eq char //micropdf417.ps eq or {
                    txtseq k get char //micropdf417.as eq {enca} {encp} ifelse addtotext
                    /k k 1 add def
                } if

                %
                % Latches to new submode
                %
                char //micropdf417.al eq {/submode //micropdf417.a def} if
                char //micropdf417.ll eq {/submode //micropdf417.l def} if
                char //micropdf417.ml eq {/submode //micropdf417.m def} if
                char //micropdf417.pl eq {/submode //micropdf417.p def} if
            } loop

            text length 2 mod 1 eq {
                submode //micropdf417.p eq {
                    /pad //micropdf417.al encp def
                    /submode //micropdf417.a def
                } {
                    %
                    % Need to check whether a ps shift can be used to pad
                    %
                    /nextecitext false def
                    % If have one or more ECIs next followed by text sequence (array)
                    i 1 add 1 seq length 1 sub {
                        /ti exch def
                        seq ti get type /arraytype ne { /nextecitext false def exit } if
                        seq ti get 0 get -1000000 gt { exit } if
                        /nextecitext true def
                    } for
                    nextecitext { % Then don't use ps as it would affect first char of text sequence
                        submode //micropdf417.a eq {
                            charvals //micropdf417.l get seq ti get 0 get known {  % Check whether to use ll or ml latch
                                /pad //micropdf417.ll enca def
                                /submode //micropdf417.l def
                            } {
                                /pad //micropdf417.ml enca def
                                /submode //micropdf417.m def
                            } ifelse
                        } {
                        submode //micropdf417.m eq {
                            charvals //micropdf417.l get seq ti get 0 get known {  % Check whether to use ll, pl or al latch
                                /pad //micropdf417.ll encm def
                                /submode //micropdf417.l def
                            } {
                            charvals //micropdf417.p get seq ti get 0 get known {
                                /pad //micropdf417.pl encm def
                                /submode //micropdf417.p def
                            } {
                                /pad //micropdf417.al encm def
                                /submode //micropdf417.a def
                            } ifelse } ifelse
                        } {  % L
                            /pad //micropdf417.ml encl def  % Only latch available
                            /submode //micropdf417.m def
                        } ifelse } ifelse
                    } {  % Otherwise use standard ps shift pad
                        /pad //micropdf417.ps textencfuncs submode get load exec def
                    } ifelse
                } ifelse
                /text [ text aload pop pad ] def
            } if

            /out text length 2 idiv array def
            0 1 out length 1 sub {
                /k exch def
                out k text k 2 mul get 30 mul text k 2 mul 1 add get add put
            } for

            out

        } def

        /encn {
            /in exch def
            mark
            0 44 in length 1 sub {
                /k exch def
                /gmod [
                    1 in k in length k sub dup 44 gt {pop 44} if getinterval
                    {48 sub} forall
                ] def
                /cwn 15 array def /cwi 15 def {  % Pre-allocate, fill from end
                    /dv 900 def
                    /val 0 def /gmulstart true def
                    mark  % Start collecting gmul digits
                    {
                        gmod length 0 eq {exit} if
                        /val val 10 mul gmod 0 get add def
                        /gmod gmod 1 gmod length 1 sub getinterval def
                        val dv lt {
                            gmulstart not { 0 } if  % Push 0 if not at start
                        } {
                            val dv idiv  % Push quotient digit
                            /gmulstart false def
                        } ifelse
                        /val val dv mod def
                    } loop
                    counttomark array astore exch pop  % Build gmul
                    /dv val def
                    /cwi cwi 1 sub def
                    cwn cwi dv put                     % Store codeword from end
                    /gmod exch def
                    gmod length 0 eq {exit} if
                } loop
                cwn cwi 15 cwi sub getinterval aload pop  % Push codewords
            } for
            counttomark array astore exch pop
        } def

        /ence {
            0 get neg 1000000 sub
                dup 000899 le {  % ECI 000000 - 000899
                927 exch 2 array astore
            } { dup 810899 le {  % ECI 000900 - 810899
                926 exch dup 900 idiv 1 sub exch 900 mod 3 array astore
            } { dup 811799 le {  % ECI 810900 - 811799
                925 exch 810900 sub 2 array astore
            } {
                pop /bwipp.pdf417badECI (PDF417 supports ECIs 000000 to 811799) //raiseerror exec
            } ifelse } ifelse } ifelse
        } def

        /encfuncs //micropdf417.encfuncs def

        /addtocws {
            dup datcws j 3 -1 roll putinterval
            length j add /j exch def
        } def

        %
        % Encode the sequence
        %
        % No need to set state as seq will always begin with a latch, unlike for pdf417
        %
        /datcws seqlen array def
        /i 0 def /j 0 def {
            i seq length ge {exit} if
            /chars seq i get def
            chars type /arraytype eq {  % Encode data according to mode
                chars 0 get -1000000 le {  % ECI
                    chars ence addtocws
                } {  % Encode data according to mode
                    chars encfuncs state get load exec addtocws
                } ifelse
            } {  % Encode the latch to new state, byte shift or macro
                [ latchcws chars get ] addtocws
                chars //micropdf417.tl eq {/state //micropdf417.t def /submode //micropdf417.a def} if
                chars //micropdf417.nl eq {/state //micropdf417.n def} if
                chars //micropdf417.bl eq chars //micropdf417.bl6 eq or {/state //micropdf417.b def} if
                chars //micropdf417.bs eq {
                    /i i 1 add def
                    seq i get encb addtocws
                } if
                chars //micropdf417.m05 eq {
                    i 1 add seq length lt {
                        seq i 1 add get //micropdf417.nl eq {  % Latch to numeric mode is implied
                            /i i 1 add def
                        } if
                    } if
                    /state //micropdf417.n def
                } if
                chars //micropdf417.m06 eq {
                    i 1 add seq length lt {
                        seq i 1 add get //micropdf417.tl eq {  % Latch to mixed text submode is implied
                            /i i 1 add def
                        } if
                    } if
                    /state //micropdf417.t def /submode //micropdf417.m def
                } if
            } ifelse
            /i i 1 add def
        } loop
        /datcws datcws 0 j getinterval def

        (input.encoding) //micropdf417.after exec

    } if


    %
    % Select metrics of an appropriate symbol
    %
    (metrics.selection) //micropdf417.before exec

    /urows rows def
    /ucols columns def
    /metrics cca {//micropdf417.ccametrics} {//micropdf417.nonccametrics} ifelse def
    metrics {
        /m exch def
        /c    m 0 get def        % Columns in symbol
        /r    m 1 get def        % Rows in symbol
        /k    m 2 get def        % Error correction codewords
        /rapl m 3 get def        % Left row address pattern
        /rapc m 4 get def        % Centre row address pattern
        /rapr m 5 get def        % Right row address pattern
        /ncws r c mul k sub def  % Data codewords
        /okay true def
        datcws length ncws gt     {/okay false def} if
        urows 0 ne urows r ne and {/okay false def} if
        ucols 0 ne ucols c ne and {/okay false def} if
        okay {exit} if
    } forall

    okay not {
        /bwipp.micropdf417noValidSymbol (Maximum length exceeded or invalid size) //raiseerror exec
    } if

    (metrics.selection) //micropdf417.after exec

    %
    % Fetch the coefficients
    %
    (ecc.coeffs) //micropdf417.before exec
    /coeffs k //micropdf417.coeffscachefetch exec def
    (ecc.coeffs) //micropdf417.after exec

    (ecc.cws) //micropdf417.before exec

    %
    % Create codewords array with one extra working space element and add padding
    %
    /m datcws length def
    /n c r mul k sub def
    /cws c r mul 1 add array def
    cws 0 datcws putinterval
    cws m [ n m sub {900} repeat ] putinterval
    cws n [ k {0} repeat 0 ] putinterval

    %
    % Derive the error codewords
    %
    0 1 n 1 sub {  % i
        cws exch get cws n get add 929 mod       % t = (cws[i] + cws[n]) mod 929
        0 1 k 1 sub {  % j
            cws n 2 index add 2 copy 1 add get   % ... cws n+j cws[n+j+1]
            929                                  % ... 929
            5 index                              % ... t
            coeffs k 8 -1 roll sub 1 sub get     % ... coeffs[k-j-1]
            mul 929 mod                          % p = (t * coeffs[k-j-1]) mod 929
            sub add 929 mod put                  % cws[n+j] = (cws[n+j+1] + (929 - p)) mod 929
        } for
        pop  % t
    } for
    n 1 n k add {  % i
         cws exch 2 copy get                     % ... cws i cws[i]
         929 exch sub 929 mod put                % cws[i] = (929 - cws[i]) mod 929
    } for

    %
    % Trim the working space from the end of the codewords
    %
    /cws cws 0 cws length 1 sub getinterval def

    options /debugcws known
    /uk.co.terryburton.bwipp.global_ctx dup where {exch get /enabledebug known} {pop false} ifelse
    and { /bwipp.debugcws cws //raiseerror exec } if

    (ecc.cws) //micropdf417.after exec

    (matrix.layout) //micropdf417.before exec

    %
    % Return the 17 bits for a codeword in a given cluster
    %
    /cwtobits {
        //micropdf417.clusters exch get exch get /v exch def
        [ 17 {0} repeat v 2 17 string cvrs {48 sub} forall ]
        dup length 17 sub 17 getinterval
    } def

    %
    % Return the 10 bits for a row address pattern
    %
    /raptobits {
        //micropdf417.raps exch get exch get /v exch def
        [ 10 {0} repeat v 2 10 string cvrs {48 sub} forall ]
        dup length 10 sub 10 getinterval
    } def

    %
    % Populate bitmap for the image
    %
    /rwid //micropdf417.rwids c 1 sub get def
    c 3 eq cca and {/rwid 72 def} if
    rwid r mul rowmult mul { array } stopped {
        pop /bwipp.micropdf417PixsTooLarge (The resulting symbol with the given rowmult exceeds the implementation maximum array size) //raiseerror exec
    } if /pixs exch def

    0 1 r 1 sub {
        /i exch def
        /clst i rapl add 1 sub 3 mod def
        [
            c 1 eq {
                i rapl add 1 sub 52 mod 0 raptobits aload pop
                cws i get clst cwtobits aload pop
                i rapr add 1 sub 52 mod 0 raptobits aload pop
            } if
            c 2 eq {
                i rapl add 1 sub 52 mod 0 raptobits aload pop
                cws i 2 mul get clst cwtobits aload pop
                cws i 2 mul 1 add get clst cwtobits aload pop
                i rapr add 1 sub 52 mod 0 raptobits aload pop
            } if
            c 3 eq {
                cca not {
                    i rapl add 1 sub 52 mod 0 raptobits aload pop
                } if
                cws i 3 mul get clst cwtobits aload pop
                i rapc add 1 sub 52 mod 1 raptobits aload pop
                cws i 3 mul 1 add get clst cwtobits aload pop
                cws i 3 mul 2 add get clst cwtobits aload pop
                i rapr add 1 sub 52 mod 0 raptobits aload pop
            } if
            c 4 eq {
                i rapl add 1 sub 52 mod 0 raptobits aload pop
                cws i 4 mul get clst cwtobits aload pop
                cws i 4 mul 1 add get clst cwtobits aload pop
                i rapc add 1 sub 52 mod 1 raptobits aload pop
                cws i 4 mul 2 add get clst cwtobits aload pop
                cws i 4 mul 3 add get clst cwtobits aload pop
                i rapr add 1 sub 52 mod 0 raptobits aload pop
            } if
            1
        ]
        0 1 rowmult 1 sub {
            i rowmult mul add rwid mul pixs exch 2 index putinterval
        } for
        pop
    } for

    (matrix.layout) //micropdf417.after exec

    <<
    /ren /renmatrix
    /pixs pixs
    /pixx rwid
    /pixy r rowmult mul
    /height r rowmult mul 72 div
    /width rwid 72 div
    /borderleft 1.0
    /borderright 1.0
    /bordertop 1.0
    /borderbottom 1.0
    /opt options
    >>

    _render //renmatrix if

    end

}
[/barcode] {null def} forall
bind def
/micropdf417 dup load /uk.co.terryburton.bwipp defineresource pop
(setup.define) //micropdf417.after exec
end
/setpacking where {pop setpacking} if
setglobal
% --END ENCODER micropdf417--
