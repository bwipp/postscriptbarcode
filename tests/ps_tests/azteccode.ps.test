/azteccode dup /uk.co.terryburton.bwipp findresource cvx def
/aztecrune dup /uk.co.terryburton.bwipp findresource cvx def

/aztecrune_eq_tmpl {
    3 1 roll { 0 0 aztecrune /pixs get }
    dup 3 -1 roll 1 exch put
    dup 3 -1 roll 0 exch put
    isEqual
} def

/aztecrune_er_tmpl {
    3 1 roll { 0 0 aztecrune /pixs get }
    dup 3 -1 roll 1 exch put
    dup 3 -1 roll 0 exch put
    exch isError
} def


