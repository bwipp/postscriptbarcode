/mailmark dup /uk.co.terryburton.bwipp findresource cvx def

/eq_tmpl {
    3 1 roll { 0 0 mailmark /pixs get }
    dup 3 -1 roll 1 exch put
    dup 3 -1 roll 0 exch put
    isEqual
} def

/er_tmpl {
    3 1 roll { 0 0 mailmark }
    dup 3 -1 roll 1 exch put
    dup 3 -1 roll 0 exch put
    exch isError
} def

